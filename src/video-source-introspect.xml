<?xml version="1.0" ?>
<node name="/ipcam/Media/VideoSource">
  <interface name="ipcam.Media.VideoSource">
    <property name="Resolution" type="s" access="readwrite"/>
    <property name="Framerate" type="u" access="readwrite"/>
  </interface>
  <interface name="ipcam.Media.VideoSource.Imaging">
    <property name="Brightness" type="i" access="readwrite"/>
    <property name="Contrast" type="i" access="readwrite"/>
    <property name="Chroma" type="i" access="readwrite"/>
    <property name="Saturation" type="i" access="readwrite"/>
    <property name="Sharpness" type="i" access="readwrite"/>
    <property name="IrCutFilter" type="u" access="readwrite"/>
  </interface>
  <interface name="ipcam.Media.VideoSource.Imaging.WhiteBalance">
    <property name="Mode" type="u" access="readwrite"/>
    <property name="CrGain" type="u" access="readwrite"/>
    <property name="CbGain" type="u" access="readwrite"/>
  </interface>
  <interface name="ipcam.Media.VideoSource.Imaging.WideDynamicRange">
    <property name="Mode" type="u" access="readwrite"/>
    <property name="Level" type="u" access="readwrite"/>
  </interface>
  <interface name="ipcam.Media.VideoSource.Imaging.LDC">
    <property name="Enable" type="b" access="readwrite"/>
